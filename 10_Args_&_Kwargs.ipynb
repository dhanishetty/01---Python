      "cell_type": "code",
      "source": [
        "def address(*Name, **Adress):\n",
        "  for x in Name:\n",
        "    print(x, end= \" \")\n",
        "  print()\n",
        "  for x, y in Adress.items():\n",
        "    print(x,\"=\", y, end = \"\\n\")"
      ],
      "metadata": {
        "id": "NKsL15wkQtBV"
      },
      "execution_count": null,
      "outputs": []
    },
